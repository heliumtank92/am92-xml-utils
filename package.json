{
  "name": "@am92/xml-utils",
  "version": "2.0.0",
  "description": "Interpolate XML and Convert XML to JSON",
  "sideEffects": false,
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "main": "dist/index.js",
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts"
  ],
  "dependencies": {
    "@am92/utils-string": "^2.0.2",
